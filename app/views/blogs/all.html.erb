 <h1>All posts</h1>

 <a href="/new">
   <button>Add new post</button>
 </a>

<table>
  <thead>
  <tr>
    <th>Title</th>
    <th>Body</th>
  </tr>
  </thead>
  <tbody>
  <% @blogs.each do |b| %>
      <tr>
        <td>
          <a href="/<%= b.id %>">
            <%= b.title %>
          </a>
        </td>
        <td><%= b.body %></td>
      </tr>
  <% end %>
  </tbody>
</table>

